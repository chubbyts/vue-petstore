    "vue": "^3.5.24",
    "vue-router": "^4.6.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@chubbyts/chubbyts-function-mock": "^2.0.2",
    "@eslint/js": "^9.39.1",
    "@prettier/sync": "^0.6.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/user-event": "^14.6.1",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^24.10.0",
    "@types/qs": "^6.14.0",
